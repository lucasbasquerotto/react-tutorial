{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Chrome",
			"type": "chrome",
			"request": "launch",
			"url": "https://localhost:4000",
			"webRoot": "${workspaceFolder}/src",
			// "pathMapping": {
			// 	"/": "${workspaceFolder}/src",
			// 	"/usr/app": "${workspaceFolder}"
			// },
			"sourceMapPathOverrides": {
				"webpack:///src/*": "${webRoot}/*",
				"/usr/app/src/*": "${webRoot}/*"
			}
		}
	]
}
